                <msContents n="4">
                    <!--increment n attribute for ms containing multiple ms items-->
                    <summary>A composite paper manuscript in Bengali script, consisting of four fragmentary works, individually foliated.</summary>
                    
                    <msItem n="1" defective="unknown">
                        <locus from="1r" to="10v">[ff.1r-5v] ff. 1r-10v in original foliation, ff. 3, 5-6, 8, and 9 missing.</locus>
                        
                        <title xml:lang="sa" key="work_000153">Sāmaga Sandhyoppadaviddhi</title>
                        
                        <textLang mainLang="sa" >Sanskrit.</textLang>
                    </msItem>
                    
                    <msItem n="2" defective="unknown">
                     <locus from="1r" to="3v">[ff. 6r-8v] ff. 1r-3v in original foliation.</locus>  
                     <title xml:lang="sa" key="work_00154">Anantapayapaddhati</title>
                        <textLang mainLang="sa">Sanskrit.</textLang>
                    </msItem>
                    
                    <msItem n="3" defective="unknown">
                        <locus from="2r" to="2v">[f. 9] f. 2 in original foliation.</locus>  
                        <note>Unidentified fragment in Bengali script.</note>
                        <textLang mainLang="sa">Sanskrit.</textLang>
                    </msItem>
                        
                        <msItem n="3" defective="unknown">
                            <locus from="1r" to="1v">[f. 10] f. 1 in original foliation.</locus>  
                            <note>Unidentified fragment in Bengali script.</note>
                            <textLang mainLang="sa">Sanskrit.</textLang>
                    </msItem>
                </msContents>
                
                <physDesc>
                        <objectDesc form="pothī"> 
                            <supportDesc material="paper">
                                <support>Paper.
                                </support>
                                
                                <extent xml:id="folio" n="10"> [10ff.] 
                                    <measure type="leaf" quantity="5">5ff.</measure>
                                    + <measure type="leaf" quantity="3">3ff. </measure>
                                    + <measure type="leaf" quantity="1">1f.</measure>
                                    + <measure type="leaf" quantity="1">1f.</measure>
                                
                                <dimensions type="folio" scope="most" unit="cm">
                                    <height quantity="8"> 8</height>
                                    <width quantity="34">34</width>
                                </dimensions>
                                <!--                                        ALTERNATIVELY, if you give a range use the following pattern:
                                <dimensions type="folio" scope="range" unit="cm">
                                    <height atLeast="X" atMost="Y">ca. X-Y</height>
                                    <width atLeast="X" atMost="Y">ca. X-Y</width>
                                </dimensions>-->
                                </extent> 
                                <condition>
                                    <p>Fragmentary work. The first manuscript of <title xml:lang="sa" key="work_000153">Sāmaga Sandhyoppadaviddhi</title> lacks ff. 3, 5-6, 8, and 9.</p>
                                    <p>Damaged; some folios are smudged and stained, with <term xml:lang="sa">akṣara</term>s (mostly still readable)
                                        damaged by humidity.</p>
                                </condition>
                            </supportDesc>
                        </objectDesc>
                        
                        <handDesc>
                            <handNote scope="sole" script="Bengali" medium="blackink">
                                <desc>Bengali in black ink.</desc>
                                </handNote>
                        </handDesc>
                        
                        <bindingDesc>
                            <p>Unbound. Kept in <material>cardboard</material> library box.</p>
                        </bindingDesc>
                    </physDesc>
                    
