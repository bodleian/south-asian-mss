                  <objectDesc form="pothi">
                     <supportDesc material="palm">
                        <support source="#WinternitzKeith1905">Palm-leaves, held together by two
                           wooden boards and a string passing through two holes, one at either side
                           of the central column. The whole is kept in the original cloth
                           wrapper.</support>
                        <extent source="#WinternitzKeith1905">
                           <measure type="leaf" quantity="188">188</measure>, of which the last
                           two are almost entirely blank.<dimensions type="binding" unit="cm">
                              <height/>
                              <width/>
                           </dimensions>
                        </extent>
                     </supportDesc>
                  </objectDesc>
